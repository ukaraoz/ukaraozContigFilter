{
    "ver": "1.0.0",
    "authors": [
        "ukaraoz"
    ],
    "contact": "ukaraoz@lbl.gov",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": "no-display"
    },
    "parameters": [ 
        {
            "id": "input_refs",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": [
                    "KBaseGenomeAnnotations.Assembly",
                    "KBaseSets.AssemblySet",
                    "KBaseGenomes.Genome",
                    "KBaseSets.GenomeSet",
                    "KBaseMetagenomes.BinnedContigs",
                    "KBaseSearch.GenomeSet"
                ]
            }
        },
        {
            "id": "variance_interguild",
            "optional": false,
            "advanced": true,
            "allow_multiple": false,
            "default_values": [
                "70"
            ],
            "field_type": "text",
            "text_options": {
                "validate_as": "float",
                "min_float" : 10,
                "max_float" : 90
            }
        },
        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "microTrait" ],
            "field_type": "text",
            "text_options": {
                "is_output_name": true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "ukaraozContigFilter",
            "method": "run_ukaraozContigFilter",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },
                {
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },
                {
                    "input_parameter": "input_refs",
                    "target_property": "input_refs",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "variance_interguild",
                    "target_property": "variance_interguild"
                },
                {
                    "constant_value": 8,
                    "target_property": "processors"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },
                {
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}